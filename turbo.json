{
	"$schema": "https://turborepo.org/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".cache/tsbuildinfo.json"]
		},
		"dev": {
			"dependsOn": ["^dev"],
			"cache": false,
			"persistent": false
		},
		"typecheck": {
			"dependsOn": ["^build"],
			"outputs": [".cache/tsbuildinfo.json"]
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"push": {
			"cache": false,
			"interactive": true
		},
		"//#lint": {
			"cache": false
		},
		"//#sherif": {
			"cache": false
		},
		"//#knip": {
			"cache": false
		}
	},
	"globalEnv": ["WWW_URL", "DATABASE_URL"]
}
