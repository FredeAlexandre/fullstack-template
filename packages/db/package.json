{
  "name": "@acme/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./src/schema.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache dist node_modules",
    "dev": "tsc --watch",
    "push": "pnpm with-env drizzle-kit push",
    "check": "pnpm with-env tsx ./check.ts",
    "flush": "pnpm with-env tsx ./flush.ts",
    "seed": "pnpm with-env tsx ./seed.ts",
    "reset": "pnpm with-env tsx ./reset.ts",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "drizzle-zod": "^0.5.1",
    "pg": "^8.13.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/typescript-config": "workspace:*",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.28.1",
    "drizzle-seed": "^0.1.3",
    "tsx": "^4.19.2",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "drizzle-orm": "catalog:"
  }
}
